pipeline {
    agent any

    stages {
        stage('Build image') {
            steps {
                git url: 'git@github.com:mkutsybala/EPAM-Project-v3.git', branch: 'dev', credentialsId: 'Jenkins_KEY'
                echo 'Building..'
                sh "ls"
            }
        }
        stage('Build inftastructure') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('Destroy infrastructure') {
            steps {
                echo 'Destroying....'
            }
        }
    }
}